{
  "plugins": ["plugins/markdown", "plugins/summarize","node_modules/better-docs/","node_modules/better-docs/typescript"],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "better-docs": {
      "name": "My React components"
    },
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "template": "node_modules/better-docs",
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  }
}
